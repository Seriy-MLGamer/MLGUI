/**
 * class Object;
 * A base class for all other classes.
 * @return an instance of Object.
 */
fn MLG_Object_new()
{
	$self=new(1, 1, INT8)
	$self.f=MLG_Object

	ret($self)
}

fn MLG_Object_remove($self)
{
	remove($self)
}

MLG_Object=new(1, 1, INT8)

MLG_Object.remove=MLG_Object_remove